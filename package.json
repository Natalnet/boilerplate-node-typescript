	"description": "",
	"main": "server.js",
	"scripts": {
		"test": "jest --no-cache",
		"start": "nodemon --watch './' --exec \"ts-node server.ts\" -e ts",
		"build": "tsc",
		"prod": "npm run build && node ./dist/server.js"
	},
	"keywords": [],
	"author": "Marcus Dantas",
	"license": "ISC",
	"dependencies": {
		"@types/body-parser": "1.17.0",
		"@types/dotenv": "6.1.0",
		"@types/express": "4.16.1",
		"@types/jest": "24.0.9",
		"@types/module-alias": "2.0.0",
		"@types/mongoose": "5.3.20",
		"@types/morgan": "1.7.35",
		"@types/supertest": "2.0.7",
		"body-parser": "1.18.3",
		"consign": "0.1.6",
		"dotenv": "6.2.0",
		"express": "4.16.4",
		"jest": "24.1.0",
		"module-alias": "2.2.0",
		"mongoose": "5.4.17",
		"morgan": "1.9.1",
		"nodemon": "1.18.10",
		"simple-node-logger": "^18.12.22",
		"supertest": "3.4.2",
		"ts-jest": "24.0.0",
		"ts-node": "8.0.2",
		"typegoose": "5.6.0",
		"typescript": "3.3.3333"
	},
	"_moduleAliases": {
		"controllers": "./src/controllers",
		"middlewares": "./src/middlewares",
		"models": "./src/models",
		"routes": "./src/routes",
		"app": "./src",
		"utils": "./src/utils",
		"validators": "./src/validators",
		"config": "./config",
		"modules": "./src/modules"
	},
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "\\.test\\.ts",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
